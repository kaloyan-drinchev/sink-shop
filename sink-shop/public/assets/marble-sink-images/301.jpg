  <rect x="170" y="170" width="260" height="110" fill="#FAF7F0" opacity="0.7"/>
  
  <!-- Marble veining -->
  <path d="M150 200 Q250 180 350 200 Q400 220 450 210" stroke="#E2D6C7" stroke-width="2" fill="none" opacity="0.8"/>
  <path d="M180 250 Q280 240 380 250 Q430 260 450 255" stroke="#D4C4B0" stroke-width="1" fill="none" opacity="0.6"/>
  <path d="M150 180 Q200 170 300 180 Q400 190 450 185" stroke="#C9B896" stroke-width="1" fill="none" opacity="0.5"/>
  
