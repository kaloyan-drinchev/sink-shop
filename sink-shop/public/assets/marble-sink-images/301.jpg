  <circle cx="300" cy="225" r="5" fill="#C9B896"/>
  
  <!-- Text -->
  <text x="300" y="350" text-anchor="middle" fill="#8B7355" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Cream Marble Square - Type 3.1</text>
