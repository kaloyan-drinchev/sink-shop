<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="#1A1A1A"/>
  
  <!-- Black marble base -->
  <rect x="150" y="160" width="300" height="80" fill="#2D3748" opacity="0.9"/>
  <rect x="170" y="170" width="260" height="60" fill="#4A5568" opacity="0.7"/>
  
  <!-- Marble veining -->
  <path d="M150 180 Q250 175 350 180 Q400 185 450 180" stroke="#718096" stroke-width="2" fill="none" opacity="0.8"/>
  <path d="M150 220 Q250 215 350 220 Q400 225 450 220" stroke="#A0AEC0" stroke-width="1" fill="none" opacity="0.6"/>
  
  <!-- Rectangular sink bowl -->
  <rect x="200" y="170" width="200" height="60" rx="8" fill="#E2E8F0"/>
  <rect x="210" y="180" width="180" height="40" rx="5" fill="#F7FAFC"/>
  
  <!-- Drain -->
  <circle cx="300" cy="200" r="8" fill="#2D3748"/>
  <circle cx="300" cy="200" r="5" fill="#1A1A1A"/>
  
  <!-- Text -->
  <text x="300" y="350" text-anchor="middle" fill="#F7FAFC" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Black Marble Rectangle - Type 4.2</text>
</svg>
